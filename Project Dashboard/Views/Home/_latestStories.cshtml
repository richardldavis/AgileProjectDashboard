<h2>recently added stories</h2>
<table>
    <tr>
            <td>ID</td>
            <td>Text</td>
            <td>Priority</td>
            <td>Status</td>
            <td>Estimate</td>
            <td>Actual</td>
    </tr>
    
    @foreach (ProjectDashboard.Domain.Story story in Model.LatestStories)
    {
        <tr>
            <td>@story.ID</td>
            <td><a href="http://@story.Link">@story.Text</a></td>
            <td>@story.Priority</td>
            <td>@story.Status</td>
            <td>@story.Estimate</td>
            <td>@story.Actual</td>
        </tr>
    }
</table>